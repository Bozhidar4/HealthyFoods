    <link href="~/Content/CustomStyle.css" rel="stylesheet" />
    <link href="~/Content/flipster.css" rel="stylesheet" />
    <link href="~/Content/jquery.flipster.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.1.1.min.js"></script>
    <script src="~/Scripts/jquery.flipster.min.js"></script>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Healthy Foods", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Vegetables", "Index", "Vegetable")</li>
                    <li>@Html.ActionLink("Fruits", "Index", "Fruit")</li>
                    <li>@Html.ActionLink("Nuts", "Index", "Nut")</li>
                    <li>@Html.ActionLink("Beans", "Index", "Bean")</li>
                    <li>@Html.ActionLink("Proteins", "Index", "MeatFishEgg")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <p style="font-size: 11px">
            All values are per 100g raw product.
            Values for Protein, Carbohydrates, Fat are in grams.
            Values for Energy are in Kcal.
        </p>
        <hr />
        
        <footer>
            <p>&copy; @DateTime.Now.Year - Healthy Foods<br />
            by Bozhidar Rangelov - <a href="http://www.bozhidar-rangelov.co.uk">Portfolio</a>
<img class="myLogo" src="~/Images/favicon55.png" /></p>
        </footer>
    </div>

    

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
